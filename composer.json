{
    "name": "avto-dev/static-references-laravel",
    "description": "Static references for Laravel applications",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Avto Develop",
            "homepage": "https://github.com/avto-dev"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "avto-dev/static-references-data": "1.*",
        "laravel/framework": "~5.4"
    },
    "require-dev": {
        "laravel/laravel": "~5.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7 || ^6.4"
    },
    "autoload": {
        "psr-4": {
            "AvtoDev\\StaticReferencesLaravel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AvtoDev\\StaticReferencesLaravel\\Tests\\": "tests"
        }
    },
    "support": {
        "issues": "https://github.com/avto-dev/static-references-laravel/issues",
        "source": "https://github.com/avto-dev/static-references-laravel"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit --no-coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AvtoDev\\StaticReferencesLaravel\\StaticReferencesServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
